    "table",
    {
      "data": {
        "name": "table",
        "description": "Table displays data in tabular format.",
        "importStatement": "import { Table } from 'primeng/table';",
        "moduleImport": "import { TableModule } from 'primeng/table';",
        "basicUsage": "import { TableModule } from 'primeng/table';",
        "examples": [
          "import { TableModule } from 'primeng/table';",
          "<p-table [value]=\"products\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>{{ product.quantity }}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table [columns]=\"cols\" [value]=\"products\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header let-columns>\n        <tr>\n            <th *ngFor=\"let col of columns\">\n                {{ col.header }}\n            </th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-rowData let-columns=\"columns\">\n        <tr>\n            <td *ngFor=\"let col of columns\">\n                {{ rowData[col.field] }}\n            </td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<div class=\"flex justify-center mb-4\">\n    <p-selectbutton [options]=\"sizes\" [(ngModel)]=\"selectedSize\" [multiple]=\"false\" optionLabel=\"name\" optionValue=\"value\" />\n</div>\n<p-table [value]=\"products\" [tableStyle]=\"{ 'min-width': '50rem' }\" [size]=\"selectedSize\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>{{ product.quantity }}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table\n    [value]=\"products\"\n    showGridlines\n    [tableStyle]=\"{ 'min-width': '50rem' }\">\n        <ng-template #header>\n            <tr>\n                <th>Code</th>\n                <th>Name</th>\n                <th>Category</th>\n                <th>Quantity</th>\n            </tr>\n        </ng-template>\n        <ng-template #body let-product>\n            <tr>\n                <td>{{ product.code }}</td>\n                <td>{{ product.name }}</td>\n                <td>{{ product.category }}</td>\n                <td>{{ product.quantity }}</td>\n            </tr>\n        </ng-template>\n</p-table>",
          "<p-table [value]=\"products\" stripedRows [tableStyle]=\"{'min-width': '50rem'}\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>{{product.code}}</td>\n            <td>{{product.name}}</td>\n            <td>{{product.category}}</td>\n            <td>{{product.quantity}}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table [value]=\"products\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr [ngClass]=\"rowClass(product)\" [ngStyle]=\"rowStyle(product)\">\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>\n                <p-badge [value]=\"product.quantity\" [severity]=\"stockSeverity(product)\" />\n            </td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table\n    [value]=\"customers\"\n    [paginator]=\"true\"\n    [rows]=\"5\"\n    [tableStyle]=\"{ 'min-width': '50rem' }\"\n    [rowsPerPageOptions]=\"[5, 10, 20]\"\n>\n    <ng-template #header>\n        <tr>\n            <th style=\"width:25%\">Name</th>\n            <th style=\"width:25%\">Country</th>\n            <th style=\"width:25%\">Company</th>\n            <th style=\"width:25%\">Representative</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-customer>\n        <tr>\n            <td>{{ customer.name }}</td>\n            <td>{{ customer.country.name }}</td>\n            <td>{{ customer.company }}</td>\n            <td>{{ customer.representative.name }}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-toggleswitch [(ngModel)]=\"metaKey\" inputId=\"input-metakey\" />\n<p-table [value]=\"products\" selectionMode=\"single\" [(selection)]=\"selectedProduct\" [metaKeySelection]=\"metaKey\" dataKey=\"id\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr [pSelectableRow]=\"product\">\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>{{ product.quantity }}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<div class=\"flex justify-center items-center mb-6 gap-2\">\n    <p-toggleswitch [(ngModel)]=\"metaKey\" inputId=\"input-metakey\" />\n    <label for=\"input-metakey\">MetaKey</label>\n</div>\n<p-table [value]=\"products\" selectionMode=\"multiple\" [(selection)]=\"selectedProducts\" [metaKeySelection]=\"metaKey\" dataKey=\"code\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product let-rowIndex=\"rowIndex\">\n        <tr [pSelectableRow]=\"product\" [pSelectableRowIndex]=\"rowIndex\">\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>{{ product.quantity }}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table [value]=\"products\" [(selection)]=\"selectedProduct\" dataKey=\"code\" [tableStyle]=\"{'min-width': '50rem'}\">\n    <ng-template #header>\n        <tr>\n            <th style=\"width: 4rem\"></th>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>\n                <p-tableRadioButton [value]=\"product\" />\n            </td>\n            <td>{{product.code}}</td>\n            <td>{{product.name}}</td>\n            <td>{{product.category}}</td>\n            <td>{{product.quantity}}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table [value]=\"products\" [(selection)]=\"selectedProducts\" dataKey=\"code\" [tableStyle]=\"{'min-width': '50rem'}\">\n    <ng-template #header>\n        <tr>\n            <th style=\"width: 4rem\"><p-tableHeaderCheckbox /></th>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>\n                <p-tableCheckbox [value]=\"product\" />\n            </td>\n            <td>{{product.code}}</td>\n            <td>{{product.name}}</td>\n            <td>{{product.category}}</td>\n            <td>{{product.quantity}}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-toast />\n<p-table [value]=\"products\" [tableStyle]=\"{ 'min-width': '50rem' }\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n            <th style=\"width: 5rem\"></th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr>\n            <td>{{ product.code }}</td>\n            <td>{{ product.name }}</td>\n            <td>{{ product.category }}</td>\n            <td>{{ product.quantity }}</td>\n            <td>\n                <p-button icon=\"pi pi-search\" (click)=\"selectProduct(product)\" severity=\"secondary\" rounded />\n            </td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-toast />\n<p-table [value]=\"products\" selectionMode=\"single\" [(selection)]=\"selectedProduct\" dataKey=\"code\" (onRowSelect)=\"onRowSelect($event)\" (onRowUnselect)=\"onRowUnselect($event)\" [tableStyle]=\"{'min-width': '50rem'}\">\n    <ng-template #header>\n        <tr>\n            <th>Code</th>\n            <th>Name</th>\n            <th>Category</th>\n            <th>Quantity</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-product>\n        <tr [pSelectableRow]=\"product\">\n            <td>{{product.code}}</td>\n            <td>{{product.name}}</td>\n            <td>{{product.category}}</td>\n            <td>{{product.quantity}}</td>\n        </tr>\n    </ng-template>\n</p-table>",
          "<p-table [value]=\"customers\" [scrollable]=\"true\" scrollHeight=\"400px\" [tableStyle]=\"{'min-width': '50rem'}\">\n    <ng-template #header>\n        <tr>\n            <th>Name</th>\n            <th>Country</th>\n            <th>Company</th>\n            <th>Representative</th>\n        </tr>\n    </ng-template>\n    <ng-template #body let-customer>\n        <tr>\n            <td>{{customer.name}}</td>\n            <td>{{customer.country.name}}</td>\n            <td>{{customer.company}}</td>\n            <td>{{customer.representative.name}}</td>\n        </tr>\n    </ng-template>\n</p-table>",
