      "data": {
        "name": "tabs",
        "description": "Tabs is a container component to group content with tabs.",
        "importStatement": "import { Tabs } from 'primeng/tabs';",
        "moduleImport": "import { TabsModule } from 'primeng/tabs';",
        "basicUsage": "import { TabsModule } from 'primeng/tabs';",
        "examples": [
          "import { TabsModule } from 'primeng/tabs';",
          "<p-tabs value=\"0\">\n    <p-tablist>\n        @for (tab of tabs; track tab.value) {\n            <p-tab [value]=\"tab.value\">{{ tab.title }}</p-tab>\n        }\n    </p-tablist>\n    <p-tabpanels>\n        @for (tab of tabs; track tab.value) {\n            <p-tabpanel [value]=\"tab.value\">\n                <p class=\"m-0\">{{ tab.content }}</p>\n            </p-tabpanel>\n        }\n    </p-tabpanels>\n</p-tabs>",
          "<p-tabs value=\"0\" scrollable>\n    <p-tablist>\n        @for(tab of scrollableTabs; track tab.value){\n            <p-tab [value]=\"tab.value\">\n                {{ tab.title }}\n            </p-tab>\n        }\n    </p-tablist>\n    <p-tabpanels>\n        @for(tab of scrollableTabs; track tab.value){\n            <p-tabpanel [value]=\"tab.value\">\n                <p class=\"m-0\">{{ tab.content }}</p>\n            </p-tabpanel>\n        }\n    </p-tabpanels>\n</p-tabs>",
          "<p-tabs value=\"/dashboard\">\n    <p-tablist>\n        @for(tab of tabs; track tab.route){\n            <p-tab [value]=\"tab.route\" [routerLink]=\"tab.route\" class=\"flex items-center !gap-2 text-inherit\">\n                <i [class]=\"tab.icon\"></i>\n                <span>{{ tab.label }}</span>\n            </p-tab>\n        }\n    </p-tablist>\n</p-tabs>\n<!--<router-outlet></router-outlet>-->"
